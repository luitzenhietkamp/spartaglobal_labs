@page
@model lab_80_asp_core_web.Pages.listModel
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

<h1>Here is a list from a model in the code-behind</h1>

<table class="table table-striped table-hover table-bordered">
    <tr>
        <th>List of strings</th>
        <th>List of strings</th>
        <th>List of strings</th>
    </tr>
    @for (int i = 0; i < Model.items.Count; i++)
    {
    <tr>
        <td>@Model.items[i]</td>
        <td>@Model.items[Model.items.Count - 1 - i]</td>
        <td>@Model.items[i]</td>
    </tr>
    }
</table>