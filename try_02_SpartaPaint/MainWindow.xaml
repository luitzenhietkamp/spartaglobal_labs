<Window x:Class="try_02_SpartaPaint.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:try_02_SpartaPaint"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        MouseWheel="ZoomHandler">
    <DockPanel x:Name="root">
        <StackPanel x:Name="Taskbar" DockPanel.Dock="Top" Orientation="Horizontal" Height="20">
            <TextBlock x:Name="DeltaDisplay" />
        </StackPanel>
        <Canvas x:Name="ImageCanvas" />

    </DockPanel>
</Window>
