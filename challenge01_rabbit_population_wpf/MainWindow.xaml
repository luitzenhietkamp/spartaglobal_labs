        <StackPanel>
            <Button Height="100" Margin="20,20,20,0" x:Name="StartSimulation" Click="StartSimulation_Click">Start simulation</Button>
            <ListBox Margin="20,20,20,20" Height="450" x:Name="Listbox_Results" />

        </StackPanel>
    </Grid>
</Window>
